{
  "name": "react-pie-charts-size",
  "version": "1.0.0",
  "description": "Measure bundle sizes of the main React pie charts",
  "main": "index.js",
  "dependencies": {
    "@size-limit/preset-big-lib": "^4.4.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-minimal-pie-chart": "^6.0.1",
    "recharts": "^1.8.5",
    "size-limit": "^4.4.0",
    "victory-pie": "^34.0.0"
  },
  "scripts": {
    "size": "size-limit"
  },
  "size-limit": [
    {
      "path": "src/recharts.js",
      "ignore": [
        "react",
        "react-dom",
        "prop-types"
      ]
    },
    {
      "path": "src/victoryPie.js",
      "ignore": [
        "react",
        "react-dom",
        "prop-types"
      ]
    },
    {
      "path": "src/reactMinimalPieChart.js",
      "ignore": [
        "react",
        "react-dom",
        "prop-types"
      ]
    }
  ],
